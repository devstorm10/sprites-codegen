{
  "name": "sprites",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:backend": "npm run --prefix backend dev",
    "start:backend": "npm start --prefix backend",
    "build:backend": "npm run --prefix backend build",
    "dev:frontend": "npm run --prefix frontend dev",
    "build:frontend": "npm run --prefix frontend build",
    "lint:frontend": "npm run --prefix frontend lint",
    "storybook": "npm run --prefix frontend storybook",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "format": "prettier --write \"backend/**/*.{js,css,ts,tsx,jsx}\" \"frontend/**/*.{js,css,ts,tsx,jsx}\"",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  },
  "dependencies": {
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  }
}
